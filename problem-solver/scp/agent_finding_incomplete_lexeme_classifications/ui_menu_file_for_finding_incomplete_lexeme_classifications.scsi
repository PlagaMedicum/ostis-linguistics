ui_menu_file_for_finding_incomplete_lexeme_classifications
    <- ui_user_command_class_atom;
       ui_user_command_class_view_kb;
    => nrel_main_idtf:
        [Принадлежность к каким дополнительным категориям можно выделить у данной лексемы?] (* <- lang_ru;; *);
        [What is the completion of lexeme classification with special combinations of classes?] (* <- lang_en;; *);
    => ui_nrel_command_template:
        [*
            question_finding_incomplete_lexeme_classifications _-> ._question_finding_incomplete_lexeme_classifications_instance
            (*
                _-> ui_arg_1;;
            *);;
            ._question_finding_incomplete_lexeme_classifications_instance _<- question;;
        *];
    => ui_nrel_command_lang_template:
        [Запрос на определение возможности принадлежности лексемы $ui_arg_1 к дополнительной категории] (* <- lang_ru;; *);
        [Request to find completion of lexeme $ui_arg_1 classification with special combinations of classes] (* <- lang_en;; *);;

